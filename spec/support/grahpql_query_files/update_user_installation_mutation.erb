mutation {
  updateUserInstallation(input: {updateUserInstallation: {userId: <%= user_id %>, token: "<%= token %>", tokenType: <%= token_type %>, deviceType: <%= device_type %>, appVersion:"<%= app_version %>"}}){
    user {
      installations {
        edges {
          node{
            appVersion
            deviceType
            token{
              token
              tokenType
            }
            id
          }
        }
      }
    }
  }
}
