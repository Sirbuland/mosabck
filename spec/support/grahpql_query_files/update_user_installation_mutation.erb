mutation {
  updateUserInstallation(input: {updateUserInstallation: {userId: <%= user_id %>, token: "<%= token %>", deviceType: <%= device_type %>, appVersion:"<%= app_version %>"}}){
    user {
      installations {
        edges {
          node{
            appVersion
            deviceType
            id
          }
        }
      }
    }
  }
}
