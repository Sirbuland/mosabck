query {
  allGeoMeta(xMin: <%= coords[:x_min] %>, xMax: <%= coords[:x_max] %>, yMin: <%= coords[:y_min] %>, yMax: <%= coords[:y_max] %>, filterTypes: [<%= types.join(',') %>]) {
    edges {
      node {
        ... on Post {
          ... on TextPost {
            postBody: body
            geoMeta {
              city
            }
          }
        }
      }
    }
  }
}
